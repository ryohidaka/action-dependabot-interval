{
  "name": "action-template",
  "version": "0.0.1",
  "description": "GitHub Action Template",
  "main": "src/main.ts",
  "scripts": {
    "build": "ncc build --license licenses.txt",
    "test": "npx jest --testMatch '**/__test__/**/*.test.ts'",
    "format": "npx prettier . --write",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ryohidaka/action-template.git"
  },
  "keywords": [
    "actions",
    "node",
    "md"
  ],
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "author": "ryohidaka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ryohidaka/action-template/issues"
  },
  "homepage": "https://github.com/ryohidaka/action-template#readme",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@vercel/ncc": "^0.38.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  }
}
